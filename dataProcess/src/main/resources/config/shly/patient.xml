<?xml version="1.0" encoding="UTF-8"?>
<body>
    <table name="六院_patient" group-column="PatientNo" id-column-names="PatientNo" display-column="PatientNo,SexName,PatientName,ContactAdd,MarriageName,NationName,Career,DateOfBirth">
        <property bean-name="id" column-name="PatientNo" />
        <property bean-name="patientId" column-name="PatientNo" patient-prefix="shly_"/>
        <property bean-name="sex" column-name="SexName" />
        <property bean-name="name" column-name="PatientName" />
        <property bean-name="address" column-name="ContactAdd" />
        <property bean-name="marriage" column-name="MarriageName" />
        <property bean-name="nation" column-name="NationName" />
        <property bean-name="job" column-name="Career" />
        <property bean-name="birthDay" column-name="DateOfBirth" date-type="YYYYMMDD" formatt="YYYY-MM-DD"/>
    </table>
</body>